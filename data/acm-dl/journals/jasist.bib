@Article{	  10.1002/asi.21557,
  author	= {Egghe_aff1n2, Leo and Guns, Raf and Rousseau_aff1n2n3,
		  Ronald},
  title		= {Thoughts on Uncitedness: Nobel Laureates and Fields
		  Medalists as Case Studies},
  year		= {2011},
  issue_date	= {August 2011},
  publisher	= {John Wiley &amp; Sons, Inc.},
  address	= {USA},
  volume	= {62},
  number	= {8},
  issn		= {1532-2882},
  url		= {https://doi.org/10.1002/asi.21557},
  doi		= {10.1002/asi.21557},
  abstract	= {Contrary to what one might expect, Nobel laureates and
		  Fields medalists have a rather large fraction (10% or more)
		  of uncited publications. This is the case for (in total) 75
		  examined researchers from the fields of mathematics (Fields
		  medalists), physics, chemistry, and physiology or medicine
		  (Nobel laureates). We study several indicators for these
		  researchers, including the h-index, total number of
		  publications, average number of citations per publication,
		  the number (and fraction) of uncited publications, and
		  their interrelations. The most remarkable result is a
		  positive correlation between the h-index and the number of
		  uncited articles. We also present a Lotkaian model, which
		  partially explains the empirically found regularities. ©
		  2011 Wiley Periodicals, Inc.},
  journal	= {J. Am. Soc. Inf. Sci. Technol.},
  month		= aug,
  pages		= {1637–1644},
  numpages	= {8},
  keywords	= {Lotka's law, bibliographic citations, author productivity,
		  citation analysis, scholars}
}

@Article{	  10.1002/asi.21556,
