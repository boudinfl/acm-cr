@Article{	  10.5555/1752166.1752188,
  author	= {Xia, Jingfeng},
  title		= {A Longitudinal Study of Scholars Attitudes and Behaviors
		  toward Open-Access Journal Publishing},
  year		= {2010},
  issue_date	= {March 2010},
  publisher	= {John Wiley &amp; Sons, Inc.},
  address	= {USA},
  volume	= {61},
  number	= {3},
  issn		= {1532-2882},
  abstract	= {This research applies statistical time series analysis to
		  examine the changing pattern of scholars' attitudes toward
		  open-access (OA) journal publishing from the early 1990s.
		  By synthesizing survey results in existing studies, this
		  research focuses on representative aspects of the attitudes
		  and behaviors recorded through the years. It finds that
		  although an increase in the publishing and awareness rates
		  of scholars with regard to OA journals has been observed,
		  scholars have been consistently concerned with the low
		  prestige of such journals and their lack of peer review,
		  which is not the case in practice. It is hoped that the
		  findings will provide useful information for the
		  improvement of OA advocacy. © 2010 Wiley Periodicals,
		  Inc.},
  journal	= {J. Am. Soc. Inf. Sci. Technol.},
