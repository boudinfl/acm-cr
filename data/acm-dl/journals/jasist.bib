@Article{	  10.1002/asi.22632,
  author	= {Hu, Xiaojun and Rousseau, Ronald and Chen, Jin},
  title		= {A New Approach for Measuring the Value of Patents Based on
		  Structural Indicators for Ego Patent Citation Networks},
