}

@Article{	  10.5555/1458620.1458636,
  author	= {Lakshminarayana, Sadasivuni},
  title		= {Anti-Plagiarism Certification Be an Academic Mandate},
  year		= {2008},
  issue_date	= {November 2008},
  publisher	= {John Wiley &amp; Sons, Inc.},
  address	= {USA},
