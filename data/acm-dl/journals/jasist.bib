  journal	= {J. Am. Soc. Inf. Sci. Technol.},
  month		= mar,
  pages		= {558–573},
  numpages	= {16},
  keywords	= {search strategies, children, search behavior, user
		  attributes, user studies}
}

@Article{	  10.1002/asi.21695,
