@Article{	  10.5555/1464476.1464489,
  author	= {J\"{a}rvelin, Kalervo and Persson, Olle},
  title		= {Erratum Re: “The DCI-Index: Discounted Cumulated
		  Impact-Based Research Evaluation”, Journal of the
		  American Society for Information Science and Technology,
		  59(9), 1433-1440},
  year		= {2008},
  issue_date	= {December 2008},
  publisher	= {John Wiley &amp; Sons, Inc.},
  address	= {USA},
  volume	= {59},
  number	= {14},
  issn		= {1532-2882},
  abstract	= {The article by K. J\"{a}rvelin &amp; O. Persson published
		  in JASIST 59(9), “The DCI-Index: Discounted Cumulated
		  Impact-Based Research Evaluation,” (pp. 1433–1440)
		  contains an unfortunate error in one of its formulas,
		  Equation 3. The present paper gives the correction and an
