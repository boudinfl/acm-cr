@Article{	  10.1002/asi.21644,
  author	= {Wu, Mei-Mei and Liu, Ying-Hsang},
  title		= {On Intermediaries' Inquiring Minds, Elicitation Styles,
		  and User Satisfaction},
  year		= {2011},
  issue_date	= {December 2011},
  publisher	= {John Wiley &amp; Sons, Inc.},
  address	= {USA},
  volume	= {62},
  number	= {12},
  issn		= {1532-2882},
  url		= {https://doi.org/10.1002/asi.21644},
  doi		= {10.1002/asi.21644},
  abstract	= {Building upon previous research on the concepts of
		  inquiring minds and elicitation styles (Wu, 2005; Wu &amp;
		  Liu, 2003), this study aims to identify the relationships
		  between the theoretical constructs of elicitation behavior
		  and user satisfaction in terms of the relevance, utility,
		  and satisfaction of search results, search interaction
		  processes, and overall search activities. Descriptive
		  statistical analysis is applied to compare the user
		  satisfaction ratings with respect to the concepts of
		  inquiring minds and elicitation styles. The results suggest
		  that the stereotyped elicitation style received the lowest
		  user satisfaction ratings compared with functionally and
		  situationally oriented styles. It is suggested that the
		  intermediaries take into account the characteristics of
		  search questions and, accordingly, adapt their professional
		  mindsets to search interview situations; that is, using an
		  inquiring mind in the query formulation process as default
		  mode with functional and situational styles of elicitations
		  would be helpful for enhancing the user's satisfaction
		  ratings. Future research is suggested to better understand
		  and to improve professional talk in information services.
		  © 2011 Wiley Periodicals, Inc.},
  journal	= {J. Am. Soc. Inf. Sci. Technol.},
  month		= dec,
  pages		= {2396–2403},
  numpages	= {8},
  keywords	= {presearch interviews, user satisfaction, intermediaries,
		  reference interviews, communication skills}
}

@Article{	  10.1002/asi.21643,
  author	= {Salovaara, Antti and Helfenstein, Sacha and Oulasvirta,
		  Antti},
  title		= {Everyday Appropriations of Information Technology: A Study
		  of Creative Uses of Digital Cameras},
  year		= {2011},
  issue_date	= {December 2011},
  publisher	= {John Wiley &amp; Sons, Inc.},
  address	= {USA},
  volume	= {62},
  number	= {12},
  issn		= {1532-2882},
  url		= {https://doi.org/10.1002/asi.21643},
  doi		= {10.1002/asi.21643},
  abstract	= {Repurposive appropriation is a creative everyday act in
		  which a user invents a novel use for information technology
		  (IT) and adopts it. This study is the first to address its
		  prevalence and predictability in the consumer IT context.
		  In all, 2,379 respondents filled in an online questionnaire
		  on creative uses of digital cameras, such as using them as
		  scanners, periscopes, and storage media. The data reveal
		  that such creative uses are adopted by about half of the
		  users, on average, across different demographic
		  backgrounds. Discovery of a creative use on one's own is
		  slightly more common than is learning it from others. Most
		  users discover the creative uses either completely on their
		  own or wholly through learning from others. Our regression
		  model explains 34% of the variance in adoption of invented
		  uses, with technology cognizance orientation, gender,
		  exploration orientation, use frequency, and use tenure as
		  the strongest predictors. These findings have implications
		  for both design and marketing. © 2011 Wiley Periodicals,
		  Inc.},
  journal	= {J. Am. Soc. Inf. Sci. Technol.},
  month		= dec,
  pages		= {2347–2363},
  numpages	= {17},
  keywords	= {repurposing, creativity, innovation, cameras, user
		  attributes}
}

