}

@Article{	  10.5555/1541765.1541777,
  author	= {Tann, Chadwyn and Sanderson, Mark},
  title		= {Are Web-Based Informational Queries Changing?},
  year		= {2009},
  issue_date	= {June 2009},
  publisher	= {John Wiley &amp; Sons, Inc.},
  address	= {USA},
  volume	= {60},
  number	= {6},
  issn		= {1532-2882},
  abstract	= {This brief communication describes the results of a
		  questionnaire examining certain aspects of the Web-based
		  information seeking practices of university students. The
		  results are contrasted with past work showing that queries
		  to Web search engines can be assigned to one of a series of
		  categories: navigational, informational, and transactional.
		  The survey results suggest that a large group of queries,
		  which in the past would have been classified as
		  informational, have become at least partially navigational.
		  We contend that this change has occurred because of the
		  rise of large Web sites holding particular types of
		  information, such as Wikipedia and the Internet Movie
		  Database. © 2009 Wiley Periodicals, Inc.},
  journal	= {J. Am. Soc. Inf. Sci. Technol.},
  month		= jun,
  pages		= {1290–1293},
  numpages	= {4},
  keywords	= {online searching, search behavior, search engines, known
		  item searching, navigation}
}

