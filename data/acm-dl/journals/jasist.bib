@Article{	  10.5555/1458620.1458621,
  author	= {Egghe, Leo},
  title		= {A Model for the Size-Frequency Function of Coauthor
		  Pairs},
  year		= {2008},
  issue_date	= {November 2008},
  publisher	= {John Wiley &amp; Sons, Inc.},
  address	= {USA},
  volume	= {59},
  number	= {13},
  issn		= {1532-2882},
  abstract	= {Lotka's law was formulated to describe the number of
		  authors with a certain number of publications. Empirical
		  results (Morris &amp; Goldstein, 2007) indicate that
		  Lotka's law is also valid if one counts the number of
		  publications of coauthor pairs. This article gives a simple
		  model proving this to be true, with the same Lotka
		  exponent, if the number of coauthored papers is
		  proportional to the number of papers of the individual
		  coauthors. Under the assumption that this number of
		  coauthored papers is more than proportional to the number
		  of papers of the individual authors (to be explained in the
		  article), we can prove that the size-frequency function of
		  coauthor pairs is Lotkaian with an exponent that is higher
		  than that of the Lotka function of individual authors, a
		  fact that is confirmed in experimental results. © 2008
		  Wiley Periodicals, Inc.},
  journal	= {J. Am. Soc. Inf. Sci. Technol.},
  month		= nov,
  pages		= {2133–2137},
  numpages	= {5},
  keywords	= {Lotka's law, analytic models, null, joint authorship,
		  author productivity}
}

