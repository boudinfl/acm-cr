@inproceedings{10.1145/1460563.1460609,
author = {Burke, Moira and Kraut, Robert},
title = {Mind Your Ps and Qs: The Impact of Politeness and Rudeness in Online Communities},
year = {2008},
isbn = {9781605580074},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1460563.1460609},
doi = {10.1145/1460563.1460609},
abstract = {Little is known about the impact of politeness in online communities. We use an inductive approach to automatically model linguistic politeness in online discussion groups and determine the impact of politeness on desired outcomes, such as increased reply rates. We describe differences in perceived politeness across a variety of groups and find that, controlling for group norms of responsiveness and message length, politeness increases reply rates in some technical groups, but rudeness is more effective in some political groups. The perceived politeness scores will be used to validate linguistic politeness strategies from theory and to inform the creation of a machine learning model of linguistic politeness that can be applied as a "politeness checker" to educate newcomers to write in ways likely to elicit response from specific communities or as a rudeness detection tool for moderators.},
booktitle = {Proceedings of the 2008 ACM Conference on Computer Supported Cooperative Work},
pages = {281–284},
numpages = {4},
keywords = {computer-mediated communication, community responsiveness, linguistic politeness},
location = {San Diego, CA, USA},
series = {CSCW '08}
}

@inproceedings{10.1145/1460563.1460610,
author = {Avrahami, Daniel and Fussell, Susan R. and Hudson, Scott E.},
title = {IM Waiting: Timing and Responsiveness in Semi-Synchronous Communication},
year = {2008},
isbn = {9781605580074},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1460563.1460610},
doi = {10.1145/1460563.1460610},
abstract = {Responsiveness, or the time until a person responds to communication, can affect the dynamics of a conversation as well as participants' perceptions of one another. In this paper, we present a careful examination of responsiveness to instant messaging communication, showing, for example, that work-fragmentation significantly correlates with faster responsiveness. We show also that the presentation of the incoming communication significantly affects responsiveness (even more so than indicators that the communication was ongoing), suggesting the potential for dynamically influencing responsiveness. This work contributes to a better understanding of computer-mediated communication and to the design of new tools for computer-mediated communication.},
booktitle = {Proceedings of the 2008 ACM Conference on Computer Supported Cooperative Work},
pages = {285–294},
numpages = {10},
keywords = {responsiveness, interpersonal communication, availability},
location = {San Diego, CA, USA},
series = {CSCW '08}
}

@inproceedings{10.1145/1460563.1460611,
author = {Hancock, Jeffrey T. and Gee, Kailyn and Ciaccio, Kevin and Lin, Jennifer Mae-Hwah},
title = {I'm Sad You're Sad: Emotional Contagion in CMC},
year = {2008},
isbn = {9781605580074},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1460563.1460611},
doi = {10.1145/1460563.1460611},
abstract = {An enduring assumption about computer-mediated communication is that it undermines emotional understanding. The present study examined emotional communication in CMC by inducing negative affect in one condition and neutral affect in another. The results revealed that 1) participants experiencing negative affect produced fewer words, used more sad terms, and exchanged messages at a slower rate, 2) their partners were able to detect their partners emotional state, and 3) emotional contagion took place, in which partners interacting with participants in the negative affect condition had significantly less positive affect than partners in the control condition. These data support a relational view of CMC.},
booktitle = {Proceedings of the 2008 ACM Conference on Computer Supported Cooperative Work},
pages = {295–298},
numpages = {4},
keywords = {emotion, affect, computer-mediated communication},
location = {San Diego, CA, USA},
series = {CSCW '08}
}

@inproceedings{10.1145/1460563.1460612,
author = {Gill, Alastair J. and French, Robert M. and Gergle, Darren and Oberlander, Jon},
title = {The Language of Emotion in Short Blog Texts},
year = {2008},
isbn = {9781605580074},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1460563.1460612},
doi = {10.1145/1460563.1460612},
abstract = {Emotion is central to human interactions, and automatic detection could enhance our experience with technologies. We investigate the linguistic expression of fine-grained emotion in 50 and 200 word samples of real blog texts previously coded by expert and naive raters. Content analysis (LIWC) reveals angry authors use more affective language and negative affect words, and that joyful authors use more positive affect words. Additionally, a co-occurrence semantic space approach (LSA) was able to identify fear (which naive human emotion raters could not do). We relate our findings to human emotion perception and note potential computational applications.},
booktitle = {Proceedings of the 2008 ACM Conference on Computer Supported Cooperative Work},
pages = {299–302},
numpages = {4},
keywords = {blogs, text analysis, emotion, affect, language},
location = {San Diego, CA, USA},
series = {CSCW '08}
}

