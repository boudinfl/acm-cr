abstract = {Tactile Teacher is a pair of fingerless gloves that senses a piano teacher's finger tapping and actuates correspond-ing vibration motors on the student's glove. In this paper, we briefly introduce the gloves and report preliminary re-sults from a user study with 13 subjects. The study shows that the system improves the playing accuracy of subjects without musical instrument experience by roughly 13%. However, no significant effects on the subjects with musi-cal instrument experience were observed. We conclude the paper with future works and the potential impacts of Tactile Teacher on real-time active learning.},
booktitle = {Proceedings of the 19th ACM Conference on Computer Supported Cooperative Work and Social Computing Companion},
pages = {329–332},
numpages = {4},
keywords = {Education and learning, User study design, Mobile and embedded devices, Piano learning},
