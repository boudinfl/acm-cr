@inproceedings{10.1145/2818052.2869125,
author = {Gordon, Eric and Michelson, Becky and Haas, Jason},
title = {@Stake: A Game to Facilitate the Process of Deliberative Democracy},
year = {2016},
isbn = {9781450339506},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/2818052.2869125},
