publisher = {South African Institute for Computer Scientists and Information Technologists},
address = {ZAF},
abstract = {The tf.idf family of vector based information retrieval schemes is very popular due to its simplicity and robustness, as well as its tractability to enhancements. This paper proposes a method to automatically perform a search for new tf.idf like schemes using genetic programming. The results of this automated search are then evaluated in a simple usage scenario. Also evaluated are the effects of using different fitness functions in the genetic programming phase.},
booktitle = {Proceedings of the 2002 Annual Research Conference of the South African Institute of Computer Scientists and Information Technologists on Enablement through Technology},
pages = {224–234},
numpages = {11},
keywords = {genetic programming, tf.idf, information retrieval},
location = {Port Elizabeth, South Africa},
