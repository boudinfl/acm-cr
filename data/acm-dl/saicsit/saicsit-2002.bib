}

@inproceedings{10.5555/581506.581553,
author = {Joubert, Pieter},
title = {Information System Architecture: A Case of Mixed Metaphors},
year = {2002},
isbn = {1581135963},
publisher = {South African Institute for Computer Scientists and Information Technologists},
address = {ZAF},
abstract = {Many metaphors from other disciplines are used in the field of information systems. Through the years software engineering, business process reengineering, software architecture and other concepts have drawn from fields like engineering and architecture.The one use of the term concerns the whole organization while the other concerns individual systems. These two concepts should be distinguished by introducing the concepts of town planning to stand next to architecture. This can help information system academics and practitioners better understand the current concept of information system architecture because many lessons can be learnt from the field of town planning and applied to information systems.The parallels between architecture and information systems have been studied in detail by various authors. Certain authors describes architecture as a reference discipline for MIS because both are design fields, both involve designers and users and both use technology to create an artifact to serve users.The parallels between town planning and information systems, on the other hand, have not been studied at all. This paper's main premise is that we should make a clear distinction between the high-level planning of systems on an organizational level and the high-level planning on a systems level. Specific systems can be compared to individual buildings and the organization can be compared to a town. Maybe the metaphor of town (or urban) planning should be added to our vocabulary.The benefits of this would be to clearly highlight the fundamental differences between the two areas. Many lessons can also be learned from the built environment, which includes town planning and architecture. For instance, in the built environment there is a clear distinction between different roles and disciplines. An architect, town planner and builder have different training paths and very specific roles that they play. This is something that the IT world could consider more seriously.},
booktitle = {Proceedings of the 2002 Annual Research Conference of the South African Institute of Computer Scientists and Information Technologists on Enablement through Technology},
pages = {256},
numpages = {1},
keywords = {information systems planning, metaphors, information system architecture},
location = {Port Elizabeth, South Africa},
series = {SAICSIT '02}
}

@inproceedings{10.5555/581506.581554,
author = {Kok, Dirk and Wesson, Janet},
title = {Designing Transaction Processing Systems: A Patterns Approach},
year = {2002},
isbn = {1581135963},
publisher = {South African Institute for Computer Scientists and Information Technologists},
address = {ZAF},
abstract = {This paper discusses the role of HCI design patterns for
software development in the transaction processing (TP) application
domain. The concept of a design pattern, whilst popular in several
other fields including architecture and software engineering is
relatively new to human-computer interaction (HCI). The potential
of pattern languages as a vehicle for the dissemination of HCI
design knowledge is becoming widely recognized within the HCI
community [Tidwell 1998; Van Welie 2001; Wesson 2001]. The TP
application domain has its own set of unique design problems. User
interface (UI) designers are faced with design challenges that are
specific to this application domain. This paper discusses the
development of a pattern language for transaction processing
systems (TPS) that has been used to successfully design several
TPS.TPS refers to a class of programs that facilitates and manages
transaction-oriented applications, typically for data entry and
retrieval transactions. TPS are prevalent in most areas of the
construction and engineering industries, from maintaining stock
levels in an electronics repair shop, to recording time and
attendance information for a large engineering concern. The
Transaction Processing Pattern Language (PL) discussed in this
paper can be divided into two main categories: patterns that deal
with general UI design; and patterns that focus specifically on TP.
In the context of this paper, our discussion focuses on the TP
patterns. Specifically the patterns related to data management
(viewing, capturing and editing) are discussed.Usability is a key factor for the success of online TPS. These
systems must be designed to meet specific usability objectives and
accommodate a wide diversity of users. A heuristic evaluation was
conducted of the usability of the UI designs created using the
Transaction Processing PL [Nielsen 1994]. This usability evaluation
revealed no significant usability problems. The user interface
design was found to meet its key usability objectives: readability,
consistency, efficiency, productivity and error prevention. Pattern
languages and design patterns can represent a solution to the
design problems facing UI designers. This PL has been used to
facilitate knowledge and skills transfer from a UI design expert to
novice UI designers in a South African software company. While
considerable research must still be done to refine and extend the
proposed PL, it nevertheless represents a significant contribution
to understanding the problems facing designers of TPS and the
possible solutions.},
booktitle = {Proceedings of the 2002 Annual Research Conference of the South African Institute of Computer Scientists and Information Technologists on Enablement through Technology},
pages = {257},
numpages = {1},
keywords = {pattern language, transaction processing systems, design patterns, human-computer interaction},
location = {Port Elizabeth, South Africa},
series = {SAICSIT '02}
}

@inproceedings{10.5555/581506.581555,
author = {Weideman, Melius and Haig-Smith, Timothy},
title = {An Investigation into Search Engines as a Form of Targeted Advert Delivery},
year = {2002},
isbn = {1581135963},
publisher = {South African Institute for Computer Scientists and Information Technologists},
address = {ZAF},
abstract = {The process of marketing encompasses three functions: to inform,
remind and persuade. The growth of the Internet signified a dawn of
a new age of marketing; a low cost form of marketing that presented
vast economies of scale. It can further be said that using the
Internet provided a business advantage [Aldridge et al. 1997].
However, the Internet's business advantage is not as great anymore
as most medium and large sized companies have a web site. In the
mid and late 1990's, e-commerce companies were willing to spend
vast amounts of money on advertising, apparently without concern
for the returns on the investment. Companies who were keen to
harness the potential of the Internet soon became disillusioned
with over-hyped expectations. E-commerce companies are now reducing
expenses and investment in online marketing and technology
[Aldridge et al. 1997:162, Bonanos 2001, Mollison 2002, Joseph
&amp; Poon 2001].It stands to reason that a more targeted approach would be more
efficient and cost effective. Customizing of advertising messages
to be appropriate for individuals receiving the information would
reduce resources required to generate revenue. The main benefit of
this marketing technique is that advertisers are able to target
individuals with advertising that is related to their interests or
needs. However, a major issue with Targeted Marketing is privacy.
The reason is that a collection of information is required about
individuals in order to make it effective. The more detailed the
information, the more accurate the targeting. The use of targeted
marketing does not guarantee commercial success as many other
factors are involved; there are numerous examples, which prove this
to be the case [Bonanos 2001].Search engines currently play a significant role in targeted
online marketing. It is even claimed to be highly successful for
advertisers. Although this may be a general belief, little
investigative research has been done on this topic. Related areas,
which this paper will attempt to investigate, include search
engines and their ability and effectiveness of delivering targeted
advertising. In summary, the problem addressed here is value of
Internet marketing. It would appear that marketing via search
engines is not a straightforward process.This paper is an investigation into Keyword Targeted Marketing
as provided by most search engines. Issues that are discussed
include the reasons for using this form of advertising, the media
used in the advert presentation, the various levels of accuracy in
matching keywords to banner advertisements, the metrics used to
evaluate performance and challenges that have been discovered in
its implementation. A company that wants to increase its exposure
to potential clients via search engine usage, may do so by 'buying'
certain keywords which describe their business. Usage of any of
these keywords by a potential client of this search engine will
result in a pop-up advertisement on the user's screen. Marketing
and advertising on Internet websites have entered an exiting new
era with many possibilities to be explored. However, some less
invasive measures to explore these possibilities might have to be
found.},
booktitle = {Proceedings of the 2002 Annual Research Conference of the South African Institute of Computer Scientists and Information Technologists on Enablement through Technology},
pages = {258},
numpages = {1},
keywords = {interface, internet, search engine, searching},
location = {Port Elizabeth, South Africa},
series = {SAICSIT '02}
}

@inproceedings{10.5555/581506.581557,
author = {Tunzelana, Sibongiseni},
title = {An Investigation into the Convergence of Information Technology, Telecommunications and Media in South Africa},
year = {2002},
isbn = {1581135963},
publisher = {South African Institute for Computer Scientists and Information Technologists},
address = {ZAF},
abstract = {For a long time technological sectors have been working in isolation, and in competition. The three sectors of focus, namely, telecommunications, media, and information technology started as stand-alone sectors. Each sector had its own objectives and goals. Although, these sectors overlapped significantly from time to time, a closer inspection was hardly taken. At times, the information technology sector overlapped with the telecommunications sector, the media sector with the information technology sector, or all three. At this day and age, these sectors do more than overlap, they merge. The convergence of information and communication technologies (ICT) has led to the integration of data, voice, graphics, and video into a digital environmentThe one certainty in today's business world is change. The convergence of an increasing range of communication technologies and applications is driving demand for new and innovative communications services that supports business growth, communication, technology, and the mediaThis paper examines the coming together of different sectors and technologies to bring about innovative services and products. The research is then narrowed to the South African context. Investigation of the status of cable television and the convergence of different sectors in South Africa is inspected. A glimpse of the envisaged future of the topic of the convergence of technologies in South Africa is stated.},
booktitle = {Proceedings of the 2002 Annual Research Conference of the South African Institute of Computer Scientists and Information Technologists on Enablement through Technology},
pages = {259},
numpages = {1},
keywords = {telecommunications, information communication technology, digital divide},
location = {Port Elizabeth, South Africa},
series = {SAICSIT '02}
}

@inproceedings{10.5555/581506.581558,
author = {Pather, Shaun and Erwin, Geoff},
title = {Motivations for Research Activity of Information Technology Academics},
year = {2002},
isbn = {1581135963},
publisher = {South African Institute for Computer Scientists and Information Technologists},
address = {ZAF},
abstract = {Higher Education in South Africa at tertiary level has been
undergoing intensive investigations about restructuring by national
government in the last few years. South Africa has two distinct
types of tertiary educational institution: Technikon and
University. South Africa is seeking to build a mixed
research/teaching profile for Technikons. In January 2002, a
National Working Group recommended to the National Minister of
Education various mergers between institutions aimed at systemic
financial and academic problems. One academic problem specially
noted was the low level of research output from the Technikon
sector. This paper overviews the background to this altered mission
for Technikons in South Africa. It then specifically describes
activities at Cape Technikon to promote research activity in all
disciplines. Moving to Faculty level, this paper reports on steps
taken within the Faculty of Business Informatics towards developing
a mixed culture of research and teaching, from a teaching-dominated
past.Findings from a part of a survey of all academic staff in the
Faculty (including Information Technology academics) on motivations
for their own participation in research activity and their ideal
work mix are presented. The paper reports on two components of the
survey viz. motivational factors for participation in research
activity, and respondents' choice of work mix. A list was compiled
of motivating factors, as they existed at national, Cape Technikon
(institutional), and faculty (Business Informatics) level. A fourth
category of motivational factors, personal factors, was also
compiled. In total, fifteen factors were identified across the four
categories: national (2), institutional (3), faculty (5) and
personal (5). The second component (of the survey) that is reported
on, identifies the extent to which the Technikon academics favoured
inclusion of research activity as part of their daily work
load.Of the top five motivational factors chosen by the respondents
three factors were from the personal category, and two from the
faculty category. This suggests that lecturers were motivated by
issues that were secondary to research outcomes. Although the top
ranked factor was 'personal satisfaction', enhancement of prospects
for job promotion in academia as well as opportunities outside Cape
Technikon were also factors amongst the top five. Pressure from
authority at Dean/Head of Department level showed little response,
since it was the (equally) least chosen amongst all the factors,
with only 1 of the 17 respondents indicating that they would
succumb to such 'pressure'.In view of the responses to the work-mix section of the survey,
the authors suggest that the highly publicised initiatives and
investigations at national, institutional and faculty level over
the previous 15 months created a greater awareness of the
importance of engaging in research activity. Furthermore, no non-IT
staff chose 'research only' in 2001 or 2002. There was little
change in IT staff's choice of work mix including some research.
Only IT staff chose the 'research only' work mix as first choice in
April 2002. No non-IT person chose a 'research-only' work mix.
Almost all IT staff chose work-mixes that included some research,
both in January 2001 and April 2002. Perhaps this indicates a
latent urge to be research-active.Further analysis of the results from the other sections of the
questionnaire is still to be done. The issuing of the questionnaire
has stimulated some debate within Cape Technikon and outside. The
questionnaire, with appropriate tuning, could be applied to other
faculties and disciplines at Cape Technikon, and to sister
institutions in the Western Cape Province and nationally. The
number of respondents has been small, reflecting the size of
Faculty academic staff. A longitudinal study should be possible in
a year to estimate any further shifts to or from research activity.
More sophisticated statistical analysis would be relevant when the
number of respondents increases, especially correlation and factor
analysis.},
booktitle = {Proceedings of the 2002 Annual Research Conference of the South African Institute of Computer Scientists and Information Technologists on Enablement through Technology},
pages = {260},
numpages = {1},
keywords = {research, higher education, universities, technikons},
location = {Port Elizabeth, South Africa},
series = {SAICSIT '02}
}

