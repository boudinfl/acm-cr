@inproceedings{10.1145/3278681.3278716,
author = {Moors, Carmen and Wilken, Ilana and Calteaux, Karen and Gumede, Tebogo},
title = {Human Language Technology Audit 2018: Analysing the Development Trends in Resource Availability in All South African Languages},
year = {2018},
isbn = {9781450366472},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
