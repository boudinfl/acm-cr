pages = {466},
numpages = {1},
keywords = {pet, magnetic resonance imaging, outcomes, department of energy, manhattan project, cancer therapy, solar energy, achievements, nuclear shell model, discoveries, neutron scattering, computerized axial tomography, buckyball, archaea, research, economic impacts, human genome, rtg, science, feynman diagrams, video game, erda, history, bubble chamber, government documents, doe, accomplishments, cat, radioisotope thermoelectric generators, top quark, mri, results, energy research and development administration, aec, positron emission tomography, dna, nobel prize, superconductivity, cyclotron, success stories, atom, cosmic microwave background radiation, special collection, atomic energy commission},
location = {Pittsburgh PA, PA, USA},
series = {JCDL '08}
}

