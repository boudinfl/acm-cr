@inproceedings{10.1145/1378889.1378916,
author = {Hunter, Jane and Khan, Imran and Gerber, Anna},
title = {Harvana: Harvesting Community Tags to Enrich Collection Metadata},
year = {2008},
isbn = {9781595939982},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1378889.1378916},
doi = {10.1145/1378889.1378916},
abstract = {Collaborative, social tagging and annotation systems have exploded on the Internet as part of the Web 2.0 phenomenon. Systems such as Flickr, Del.icio.us, Technorati, Connotea and LibraryThing, provide a community-driven approach to classifying information and resources on the Web, so that they can be browsed, discovered and re-used. Although social tagging sites provide simple, user-relevant tags, there are issues associated with the quality of the metadata and the scalability compared with conventional indexing systems. In this paper we propose a hybrid approach that enables authoritative metadata generated by traditional cataloguing methods to be merged with community annotations and tags. The HarvANA (Harvesting and Aggregating Networked Annotations) system uses a standardized but extensible RDF model for representing the annotations/tags and OAI-PMH to harvest the annotations/tags from distributed community servers. The harvested annotations are aggregated with the authoritative metadata in a centralized metadata store. This streamlined, interoperable, scalable approach enables libraries, archives and repositories to leverage community enthusiasm for tagging and annotation, augment their metadata and enhance their discovery services. This paper describes the HarvANA system and its evaluation through a collaborative testbed with the National Library of Australia using architectural images from PictureAustralia.},
booktitle = {Proceedings of the 8th ACM/IEEE-CS Joint Conference on Digital Libraries},
pages = {147–156},
numpages = {10},
keywords = {folksonomy, digital collections, annotation, social tagging, ontology, metadata, harvesting},
location = {Pittsburgh PA, PA, USA},
series = {JCDL '08}
}

@inproceedings{10.1145/1378889.1378917,
author = {Tonkin, Emma and Muller, Henk L.},
title = {Semi Automated Metadata Extraction for Preprints Archives},
year = {2008},
isbn = {9781595939982},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1378889.1378917},
doi = {10.1145/1378889.1378917},
abstract = {In this paper we present a system called paperBase that aids users in entering metadata for preprints. PaperBase extracts metadata from the preprint. Using a Dublin-Core based REST API, third-party repository software populates a web form that the user can then proofread and complete. PaperBase also predicts likely key words for the preprints, based on a controlled vocabulary of keywords that the archive uses and a Bayesian classifier.We have tested the system on 12 individuals, and measured the time that it took them to enter data, and the accuracy of the entered metadata. We find that our system appears to be faster than manual entry, but a larger sample needs to be tested before it can be deemed statistically significant. All but two participants perceived it to be faster. Some metadata, in particular the title of preprints, contains significantly fewer mistakes when entered automatically; even though the automatic system is not perfect, people tend to correct mistakes that paperBase makes, but would leave their own mistakes in place.},
booktitle = {Proceedings of the 8th ACM/IEEE-CS Joint Conference on Digital Libraries},
pages = {157–166},
numpages = {10},
keywords = {preprints archives, keyword extraction, metadata},
location = {Pittsburgh PA, PA, USA},
series = {JCDL '08}
}

@inproceedings{10.1145/1378889.1378918,
author = {Lu, Xiaonan and Kahle, Brewster and Wang, James Z. and Giles, C. Lee},
title = {A Metadata Generation System for Scanned Scientific Volumes},
year = {2008},
isbn = {9781595939982},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1378889.1378918},
doi = {10.1145/1378889.1378918},
abstract = {Large scale digitization projects have been conducted at digital libraries to preserve cultural artifacts and to provide permanent access. The increasing amount of digitized resources, including scanned books and scientific publications, requires development of tools and methods that will efficiently analyze and manage large collections of digitized resources. In this work, we tackle the problem of extracting metadata from scanned volumes of journals. Our goal is to extract information describing internal structures and content of scanned volumes, which is necessary for providing effective content access functionalities to digital library users. We propose methods for automatically generating volume level, issue level, and article level metadata based on format and text features extracted from OCRed text. We show the performance of our system on scanned bound historical documents nearly two centuries old. We have developed the system and integrated it into an operational digital library, the Internet Archive, for real-world usage.},
booktitle = {Proceedings of the 8th ACM/IEEE-CS Joint Conference on Digital Libraries},
pages = {167–176},
numpages = {10},
keywords = {metadata generation, scanned journal},
location = {Pittsburgh PA, PA, USA},
series = {JCDL '08}
}

