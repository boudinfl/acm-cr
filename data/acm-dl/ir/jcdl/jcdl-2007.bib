pages = {245–246},
numpages = {2},
keywords = {digital libraries, disabilities, inclusive design, usability},
location = {Vancouver, BC, Canada},
series = {JCDL '07}
}

@inproceedings{10.1145/1255175.1255224,
author = {Buchanan, George and Gow, Jeremy and Blandford, Ann and Rimmer, Jon and Warwick, Claire},
title = {Representing Aggregate Works in the Digital Library},
year = {2007},
isbn = {9781595936448},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1255175.1255224},
doi = {10.1145/1255175.1255224},
abstract = {This paper studies the challenge of representing aggregate works such as encyclopedias, collected poems and journals in heterogenous digital library collections. Reflecting on the materials used by humanities academics, we demonstrate the varied range of aggregate types and the problems of faithfully representing this in the DL interface. Aggregates are complex and pervasive, challenge common assumptions and confuse boundaries within organisational structures. Existing DL systems can only provide imperfect representation of aggregates, and alterations to document encoding are insufficient to create a faithful reproduction of the physical library. The challenge is amplified through concrete examples, and solutions are demonstrated in a well-known DL system and related to standard DL architecture.},
booktitle = {Proceedings of the 7th ACM/IEEE-CS Joint Conference on Digital Libraries},
pages = {247–256},
numpages = {10},
keywords = {digital libraries, architecture, aggregate documents},
location = {Vancouver, BC, Canada},
series = {JCDL '07}
}

@inproceedings{10.1145/1255175.1255225,
author = {Jones, Matt and Harwood, Will and Buchanan, George and Lalmas, Mounia},
