@inproceedings{10.5555/827140.827179,
author = {Gurijala, A. R. and Deller, J. R.},
title = {A Quantified Fidelity Criterion for Parameter-Embedded Watermarking of Audio Archives},
year = {2003},
isbn = {0769519393},
publisher = {IEEE Computer Society},
address = {USA},
abstract = {A novel algorithm for speech watermarking through parametric modeling is enhanced by inclusion of a quantified fidelity criterion. Watermarking is effected through solution of a set-membership filtering (SMF) problem, subject to an l∞ fidelity criterion in the signal space. The SMF approach provides flexibility in obtaining watermark solutions that trade-off watermark robustness and stegosignal fidelity.},
booktitle = {Proceedings of the 3rd ACM/IEEE-CS Joint Conference on Digital Libraries},
pages = {237–239},
numpages = {3},
location = {Houston, Texas},
series = {JCDL '03}
}

@inproceedings{10.5555/827140.827180,
author = {S\'{a}nchez, J. Alfredo and Arias, J. An\'{\i}bal},
title = {Fourth-Phase Digital Libraries: Pacing, Linking, Annotating and Citing in Multimedia Collections},
year = {2003},
isbn = {0769519393},
publisher = {IEEE Computer Society},
address = {USA},
abstract = {We discuss the implications of the use of current multimedia collections and posit that it is possible to build what we term fourth-phase digital libraries (4PDLs). In 4PDLs users can take advantage of both the powerful audiovisual channels and the proven practices developed for media such as text. We demonstrate how various technologies can be integrated to produce a 4PDL.},
