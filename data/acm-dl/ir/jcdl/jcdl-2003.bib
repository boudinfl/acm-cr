@inproceedings{10.5555/827140.827165,
author = {Monroy, Carlos and Furuta, Richard and Mallen, Enrique},
title = {Visualizing and Exploring Picasso's World},
year = {2003},
isbn = {0769519393},
publisher = {IEEE Computer Society},
address = {USA},
abstract = {We discuss the preliminary use of a visualization tool called Interactive Timeline Viewer (ItLv) in visualizing and exploring a collection of art works by Pablo Ruiz Picasso. Our data set is composed of a subset of the On-line Picasso Project, a significantly-sized on-line art repository of the renowned Spanish artist. We also include a brief discussion about how this visualization tool can help art scholars to study and analyze an artist's life and works.},
booktitle = {Proceedings of the 3rd ACM/IEEE-CS Joint Conference on Digital Libraries},
pages = {173–175},
numpages = {3},
