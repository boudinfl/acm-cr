@inproceedings{10.1145/169059.169274,
author = {Russo, Patricia and Boor, Stephen},
title = {How Fluent is Your Interface? Designing for International Users},
year = {1993},
isbn = {0897915755},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/169059.169274},
