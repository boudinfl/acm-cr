abstract = {The paper proposes that some current problems in recruiting human factors methods to system design might be alleviated by means of a structured human factors design framework. The explicit stage-wise design scope of such a framework would support the assignment of appropriate human factors methods to specific system design needs. As an illustration, the design framework of an in-house structured human factors methodology is reviewed followed by the assignment of a set of existing human factors methods against its design stages. Subsequent steps to develop the assigned methods into a similar methodology are then described. The potential of such a methodology for facilitating human factors input is discussed.},
booktitle = {Proceedings of the SIGCHI Conference on Human Factors in Computing Systems},
pages = {549–556},
numpages = {8},
keywords = {human factors method recruitment, structured design methodology, human factors system design cycle},
location = {Monterey, California, USA},
series = {CHI '92}
}

@inproceedings{10.1145/142750.142986,
author = {Nielsen, Jakob and Bush, Rita M. and Dayton, Tom and Mond, Nancy E. and Muller, Michael J. and Root, Robert W.},
title = {Teaching Experienced Developers to Design Graphical User Interfaces},
year = {1992},
isbn = {0897915135},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/142750.142986},
doi = {10.1145/142750.142986},
abstract = {Five groups of developers with experience in the design of character-based user interfaces were taught graphical user interface design through a short workshop with a focus on practical design exercises using low-tech tools derived from the PICTIVE method. Several usability problems were found in the designs by applying the heuristic evaluation method, and feedback on these problems constituted a way to make the otherwise abstract usability principles concrete for the designers at the workshop. Based on these usability problems and on observations of the design process, we conclude that object-oriented interactions are especially hard to design and that the developers were influenced by the graphical interfaces of personal computers with which they had interacted as regular users.},
booktitle = {Proceedings of the SIGCHI Conference on Human Factors in Computing Systems},
pages = {557–564},
numpages = {8},
keywords = {GUI, PICTIVE, graphical user interfaces, education, transfer of skill, standards, heuristic evaluation, object-oriented interfaces},
location = {Monterey, California, USA},
series = {CHI '92}
}

@inproceedings{10.1145/142750.142989,
author = {Mrazek, Deborah and Rafeld, Michael},
title = {Integrating Human Factors on a Large Scale: Product Usability Champions},
year = {1992},
isbn = {0897915135},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/142750.142989},
doi = {10.1145/142750.142989},
abstract = {This paper describes how a software development division in a large corporate environment found a creative way to integrate human factors techniques into their development processes. It discusses the limitations of a single Human Factors Engineer, the needs of a typical engineer on a software project, and how these limitations and needs produced the Product Usability Champion Program.Product Usability Champions are representatives from each software project in the division who act as usability watchdogs for their respective projects. The Human Factors Engineer's responsibility is to provide support to these Champions. This support includes access to a Usability Lab, technical advice, references, consulting, classroom training, hands-on training, Usability Champion program facilitation and support, and specific project team involvement. This paper describes the program's structure, implementation, and success.},
booktitle = {Proceedings of the SIGCHI Conference on Human Factors in Computing Systems},
pages = {565–570},
numpages = {6},
keywords = {Large-scale human factors, consulting, usability lab, championing, centralized usability resources, usability toolkit},
location = {Monterey, California, USA},
series = {CHI '92}
}

