author = {Lim, K. Y. and Long, J. B.},
title = {A Method for (Recruiting) Methods: Facilitating Human Factors Input to System Design},
year = {1992},
isbn = {0897915135},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/142750.142984},
