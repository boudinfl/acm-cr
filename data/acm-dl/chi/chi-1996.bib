title = {One Dimensional Motion Tailoring for the Disabled: A User Study},
year = {1992},
isbn = {0897915135},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/142750.142876},
