@inproceedings{10.1145/358916.358998,
author = {Twidale, Michael B. and Marty, Paul F.},
title = {Coping with Errors: The Importance of Process Data in Robust Sociotechnical Systems},
year = {2000},
isbn = {1581132220},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
