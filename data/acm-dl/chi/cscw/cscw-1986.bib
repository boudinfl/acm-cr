@inproceedings{10.1145/637069.637081,
author = {Malone, Thomas W. and Grant, Kenneth R. and Lai, Kum-Yew and Rao, Ramana and Rosenblitt, David},
title = {Semi-Structured Messages Are Surprisingly Useful for Computer-Supported Coordination},
year = {1986},
isbn = {9781450373654},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/637069.637081},
doi = {10.1145/637069.637081},
abstract = {This paper argues that using a set of semi-structured message templates is surprisingly helpful in designing a variety of computer-based communication and coordination systems. Semi-structured messages can help provide automatic aids for: (1) composing messages to be sent, (2) selecting, sorting, and prioritizing messages that are received, (3) responding automatically to some messages, and (4) suggesting likely responses to other messages. The use of these capabilities is illustrated in a range of applications including electronic mail, computer conferencing, calendar management, and task tracking. The applications show how ideas from artificial intelligence (such as inheritance and production rules) and ideas from user interface design (such as interactive graphical editors) can be combined in novel ways for dealing with semi-structured messages. The final part of the paper discusses how communities can evolve a useful set of message type definitions.},
booktitle = {Proceedings of the 1986 ACM Conference on Computer-Supported Cooperative Work},
pages = {102–114},
numpages = {13},
location = {Austin, Texas},
series = {CSCW '86}
}

