@inproceedings{10.1145/637069.637115,
author = {Kraemer, Kenneth L. and King, John Leslie},
title = {Computer-Based Systems for Cooperative Work and Group Decisionmaking: Status of Use and Problems in Development},
year = {1986},
isbn = {9781450373654},
