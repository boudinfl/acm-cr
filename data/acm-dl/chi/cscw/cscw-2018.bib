@inproceedings{10.1145/3272973.3272983,
author = {Asad, Mariam},
title = {Prefigurative Design as an Alternative Approach to Civic Engagement},
year = {2018},
isbn = {9781450360180},
publisher = {Association for Computing Machinery},
