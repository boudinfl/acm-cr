abstract = {Keeping in touch and staying aware of family members' health is an inherent desire among many families. Yet, conversations with family members about health can be challenging due to a number of reasons, such as: time, physical distance, and other concerns. Given that, it merits the development of tools that aim to facilitate sustainable health information sharing in the family context. In this paper, we present PhamilyHealth. PhamilyHealth is a web-based photo sharing system for family members to share health-related photos with one another and to encourage a family-wide, sustainable, healthy lifestyle.},
booktitle = {Companion of the 2018 ACM Conference on Computer Supported Cooperative Work and Social Computing},
pages = {337–340},
