url = {https://doi.org/10.1145/3272973.3272984},
doi = {10.1145/3272973.3272984},
abstract = {A distinctive feature of online peer-production systems is that participants can behave as they wish, resulting in complex and interrelated behavioral trajectories. My dissertation examines this behavior from the perspectives of the individual as well as that of the collective. I began by studying the dynamics between individuals' behaviors and early motivations over time. I then proposed a new approach for characterizing the emergent dependencies between different types of activities that influence the probability that participants behave in certain ways. By attending the Doctoral Colloquium, I hope to exchange ideas with other students and receive feedback on these two studies.},
booktitle = {Companion of the 2018 ACM Conference on Computer Supported Cooperative Work and Social Computing},
pages = {101–104},
numpages = {4},
keywords = {motivation, peer-production, complex systems, collective behavior},
location = {Jersey City, NJ, USA},
