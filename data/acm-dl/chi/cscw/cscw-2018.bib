@inproceedings{10.1145/3272973.3274063,
author = {Kuzuoka, Hideaki and Kodama, Yuki and Xu, Jianfeng and Myodo, Emi and Harada, Etsuko and Osawa, Hirotaka},
title = {Telepresence Robot's Salutations to Trigger Informal Conversation with Teleworkers},
year = {2018},
