}

@inproceedings{10.1145/3272973.3272984,
author = {Balestra, Martina},
title = {Individual and Collective Determinants of Activity in Collaborative Production},
year = {2018},
isbn = {9781450360180},
publisher = {Association for Computing Machinery},
