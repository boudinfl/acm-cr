@inproceedings{10.1145/3272973.3274543,
author = {Fiesler, Casey and Bruckman, Amy and Kraut, Robert E. and Muller, Michael and Munteanu, Cosmin and Shilton, Katie},
title = {Research Ethics and Regulation: An Open Forum},
year = {2018},
isbn = {9781450360180},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3272973.3274543},
doi = {10.1145/3272973.3274543},
abstract = {An ongoing challenge within the diverse HCI and social computing research communities is understanding research ethics in the face of evolving technology and methods. Building upon successful town hall meetings at ACM conferences including CSCW, CHI, GROUP, and IDC, this panel will be structured to facilitate audience discussion and to collect input about current challenges and processes. It will be led by members of the SIGCHI Research Ethics Committee. We will pose open questions and invite audience discussion of practices centered around issues such as recent changes to regulatory requirements (GDPR, US Institutional Review Boards), and cultural and disciplinary differences in ethical practices. There will also be discussion of how research ethical issues should be handled in SIGCHI paper submission and review processes, and how we might create and disseminate ethics resources.},
booktitle = {Companion of the 2018 ACM Conference on Computer Supported Cooperative Work and Social Computing},
pages = {125–128},
numpages = {4},
keywords = {regulation, ethics, research ethics, gdpr, irb, practices, privacy},
location = {Jersey City, NJ, USA},
series = {CSCW '18}
}

@inproceedings{10.1145/3272973.3274544,
author = {Ellison, Nicole B. and French, Megan and Litt, Eden and Sundar, S. Shyam and Trieu, Penny},
title = {Without a Trace: How Studying Invisible Interactions Can Help Us Understand Social Media},
year = {2018},
isbn = {9781450360180},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3272973.3274544},
doi = {10.1145/3272973.3274544},
abstract = {Scholars studying social media have embraced the opportunities afforded by behavioral data captured by online tools to explore the implications of platform use for outcomes such as well-being, relationship maintenance, and perceptions of social capital. However, the prevalence of these methods demands that we consider their potential limitations and the question of how to best combine them with more traditional methods, such as self-report surveys. For this panel, scholars will share brief presentations then engage with the audience, and each other, to identify concerns, opportunities, and best practices. Guiding questions include: What is lost when we rely exclusively on click-based data? How can researchers better measure and account for "invisible" interactions such as exchanges that are triggered by social media, but occur in other channels? What principles are important to bear in mind as we attempt to capture, document, and understand contemporary social media practices?},
booktitle = {Companion of the 2018 ACM Conference on Computer Supported Cooperative Work and Social Computing},
pages = {129–132},
numpages = {4},
keywords = {social media, log data, computer-mediated communication, relational development},
location = {Jersey City, NJ, USA},
series = {CSCW '18}
}

@inproceedings{10.1145/3272973.3274545,
author = {Hui, Julie and Gerber, Elizabeth M. and Dombrowski, Lynn and Gray, Mary L. and Marcus, Adam and Salehi, Niloufar},
title = {Computer-Supported Career Development in The Future of Work},
year = {2018},
isbn = {9781450360180},
