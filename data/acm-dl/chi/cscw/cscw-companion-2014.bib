author = {Velden, Theresa and Bietz, Matthew J. and Diamant, E. Ilana and Herbsleb, James D. and Howison, James and Ribes, David and Steinhardt, Stephanie B.},
title = {Sharing, Re-Use and Circulation of Resources in Cooperative Scientific Work},
year = {2014},
