title = {Eliph: Effective Visualization of Code History for Peer Assessment in Programming Education},
year = {2017},
isbn = {9781450346887},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3022198.3023266},
doi = {10.1145/3022198.3023266},
abstract = {Peer assessment is an effective pedagogical tool in which students engage in the process of evaluating other student's work. In programming education, peer assessment involves peer code review, where students mark and give feedback other peer's code. We introduce Eliph, a web-based peer assessment tool for programming education with code history visualization. Eliph incorporates the visualization of character-level code history, selection-based history tracking and the integration of execution events. In a controlled experiment performed in an undergraduate CS course, we found that Eliph helps students understand code structure and the author's intention more clearly, and promotes higher quality of peer feedback.},
booktitle = {Companion of the 2017 ACM Conference on Computer Supported Cooperative Work and Social Computing},
pages = {33–36},
numpages = {4},
keywords = {data visualization, peer review, time series visualization, code history, peer assessment},
location = {Portland, Oregon, USA},
