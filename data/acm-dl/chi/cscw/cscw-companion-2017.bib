doi = {10.1145/3022198.3023272},
abstract = {This demo presents an instance of Inquire, a tool designed to support qualitative researchers in the early stages of research. The tool enables the search over millions of users' records to extract early insights to aid in the formulation of research strategies. The tool presents the work described in the Inquire paper by Paredes, et. al. [12] in this demo we show how queries become a part of the inductive process, enabling researchers to try multiple ideas while gaining intuition and discovering less-obvious insights. We leverage LiveJournal (LJ) as a rich source of public posts, and Google News (GN) embeddings which link formal language (e.g. "reminiscence triggers") with colloquial expressions (e.g. "music brings back memories"). Using our tool, we elicit the interplay between tool and user to enhance qualitative and social research opportunities.},
booktitle = {Companion of the 2017 ACM Conference on Computer Supported Cooperative Work and Social Computing},
pages = {29–32},
numpages = {4},
keywords = {hypothesis formation, large-scale data, exploratory, insights, qualitative research, text data, semantic},
location = {Portland, Oregon, USA},
series = {CSCW '17 Companion}
}

@inproceedings{10.1145/3022198.3023266,
