}

@inproceedings{10.1145/3022198.3026319,
author = {Exposito, Marc and Zeamer, Vicky and Maes, Pattie},
title = {Unobtrusive Note Taking: Enriching Digital Interpersonal Interactions Using Gestures},
year = {2017},
isbn = {9781450346887},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3022198.3026319},
doi = {10.1145/3022198.3026319},
abstract = {People are collaborating remotely through an increasing number of digital channels with the aid of platforms such as video conferencing and VR/AR. Despite new ways of conducting interpersonal conversations, the method and technologies associated with note taking remain the same. Moreover, note taking can be seen as distracting to all of the participants involved and disrupt the flow of conversation. Nonetheless, note taking is a significant method for people to augment their own memory by selectively categorizing information as valuable and storing it through text or audio for future reference. We propose a system that integrates video teleconferencing interactions with notes as selected through unobtrusive eye tracking cues and foot-based gestures. Our system aids in the augmentation of human memory via note taking while increasing the quality of social interaction amongst all the interaction's participants.},
booktitle = {Companion of the 2017 ACM Conference on Computer Supported Cooperative Work and Social Computing},
pages = {167–170},
numpages = {4},
keywords = {social interaction, interaction techniques and devices, cognitive offloading, foot-based interaction},
location = {Portland, Oregon, USA},
series = {CSCW '17 Companion}
}

@inproceedings{10.1145/3022198.3026343,
author = {Faklaris, Cori and Hook, Sara Anne},
title = {Attitudes About 'Fair Use' and Content Sharing in Social Media Applications},
year = {2017},
isbn = {9781450346887},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3022198.3026343},
doi = {10.1145/3022198.3026343},
abstract = {The shift to Social Networking Services (SNSs) and mobile messaging apps such as Facebook, Instagram and Snapchat that rely on User-Generated Content (UGC) has challenged notions of fair use under U.S. copyright law. It remains unclear what understandings are common among these app users regarding legal and ethical norms in reusing artistic, journalistic and other types of content outside of online remixer spaces. Our online survey of N=106 users of N=48 SNS platforms and apps measured attitudes regarding fair use under U.S. copyright law and attribution for work that is shared. Participants reported a high level of agreement with more-restrictive conditions for content publishing and reuse. However, analyses of ratings and responses to open-ended questions reveal tension between issues of intellectual integrity and intellectual property.},
booktitle = {Companion of the 2017 ACM Conference on Computer Supported Cooperative Work and Social Computing},
pages = {171–174},
numpages = {4},
keywords = {plagiarism, journalism, user-generated content, content sharing, aggregation, publishing, fair use, messaging apps, attribution, copyright, intellectual property, social media, photography, authorship, video},
location = {Portland, Oregon, USA},
series = {CSCW '17 Companion}
}

@inproceedings{10.1145/3022198.3026324,
author = {Fu, Xinyu},
title = {Eliminate Misunderstandings: Measuring Virtual Team Members as Organizational Citizens},
year = {2017},
isbn = {9781450346887},
