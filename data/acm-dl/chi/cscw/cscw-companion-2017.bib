booktitle = {Companion of the 2017 ACM Conference on Computer Supported Cooperative Work and Social Computing},
pages = {327–330},
numpages = {4},
keywords = {pervasive health, physical activity, play pattern, gamification, augmented reality game, motivation},
location = {Portland, Oregon, USA},
series = {CSCW '17 Companion}
}

@inproceedings{10.1145/3022198.3026333,
author = {Uosaki, Noriko and Yonekawa, Takahiro and Yin, Chengjiu},
title = {Supporting Out-of-Class Interaction among Learners with InCircle},
