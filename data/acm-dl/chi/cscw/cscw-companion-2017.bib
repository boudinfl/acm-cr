@inproceedings{10.1145/3022198.3026310,
author = {Singhal, Samarth and Neustaedter, Carman},
title = {BeWithMe: An Immersive Telepresence System for Distance Separated Couples},
year = {2017},
isbn = {9781450346887},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3022198.3026310},
