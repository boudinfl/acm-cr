url = {https://doi.org/10.1145/3022198.3026324},
doi = {10.1145/3022198.3026324},
abstract = {Traditionally, virtual employees have been thought to perform less Organizational Citizenship Behaviors (OCBs) than face-to-face members. Nevertheless, we argued that the existing measurement of OCBs does not cover unique aspects generating in the virtual context and thus the OCBs of virtual members indeed broadly exist. Through conducting an inductive research of 123 virtual employees and 428 office employees of one high-technology company, we eventually identified a few unique virtual-featured OCBs and compared the OCBs in globally distributed teams and on-site teams respectively. Surprisingly, we also found the behaviors of virtual members generally have larger impacts on the team, than on-site colleagues do. The ongoing research is currently developing a scale of OCBs in virtual context, which would serve for future explorations.},
booktitle = {Companion of the 2017 ACM Conference on Computer Supported Cooperative Work and Social Computing},
pages = {175–178},
numpages = {4},
keywords = {experience sampling method, virtual teams, clustering, organizational citizenship behaviors, measurement},
location = {Portland, Oregon, USA},
series = {CSCW '17 Companion}
}

@inproceedings{10.1145/3022198.3026306,
