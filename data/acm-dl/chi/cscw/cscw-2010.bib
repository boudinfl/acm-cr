address = {New York, NY, USA},
url = {https://doi.org/10.1145/1718918.1718964},
doi = {10.1145/1718918.1718964},
abstract = {Employee blogging has benefits both for individuals and the organization. In order to inspire the creation of blog posts, we developed a novel topic suggestion system that connects blog readers with blog writers through sharing topics of interest. We describe our system and the results from an employee survey that informed its design.},
booktitle = {Proceedings of the 2010 ACM Conference on Computer Supported Cooperative Work},
pages = {237–240},
numpages = {4},
keywords = {blog, social software, participation, recommendations},
