@inproceedings{10.1145/1718918.1718967,
author = {Cao, Xiang and Lindley, Si\^{a}n E. and Helmes, John and Sellen, Abigail},
title = {Telling the Whole Story: Anticipation, Inspiration and Reputation in a Field Deployment of TellTable},
year = {2010},
isbn = {9781605587950},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1718918.1718967},
doi = {10.1145/1718918.1718967},
abstract = {We present a field study of TellTable, a new storytelling system designed to support creativity and collaboration amongst children. The application was deployed on a multi-touch interactive table in the library of a primary school, where children could use it to create characters and scenery based on elements of the physical world (captured through photography) as well as through drawing. These could then be used to record a story which could be played back. TellTable allowed children to collaborate in devising stories that mixed the physical and the digital in creative ways and that could include themselves as characters. Additionally, the field deployment illustrated how children took inspiration from one another's stories, how they planned elements of their own tales before using the technology, and how the fact that stories could be accessed in the library led some to become well-known and popular within the school community. The real story here, we argue, needs to take into account all that happens within the wider context of use of this system.},
booktitle = {Proceedings of the 2010 ACM Conference on Computer Supported Cooperative Work},
pages = {251–260},
numpages = {10},
keywords = {storytelling, play, community, sharing, creativity, field studies, children, interactive tabletop, collaboration},
location = {Savannah, Georgia, USA},
series = {CSCW '10}
}

