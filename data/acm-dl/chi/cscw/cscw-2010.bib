@inproceedings{10.1145/1718918.1718961,
author = {Gilbert, Eric and Karahalios, Karrie},
title = {Understanding Deja Reviewers},
year = {2010},
isbn = {9781605587950},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1718918.1718961},
doi = {10.1145/1718918.1718961},
abstract = {People who review products on the web invest considerable time and energy in what they write. So why would someone write a review that restates earlier reviews? Our work looks to answer this question. In this paper, we present a mixed-method study of deja reviewers, latecomers who echo what other people said. We analyze nearly 100,000 Amazon.com reviews for signs of repetition and find that roughly 10-15% of reviews substantially resemble previous ones. Using these algorithmically-identified reviews as centerpieces for discussion, we interviewed reviewers to understand their motives. An overwhelming number of reviews partially explains deja reviews, but deeper factors revolving around an individual's status in the community are also at work. The paper concludes by introducing a new idea inspired by our findings: a self-aware community that nudges members toward community-wide goals.},
booktitle = {Proceedings of the 2010 ACM Conference on Computer Supported Cooperative Work},
pages = {225–228},
numpages = {4},
keywords = {online communities, text processing, reviewing communities, ecommerce, product reviews},
location = {Savannah, Georgia, USA},
series = {CSCW '10}
}

@inproceedings{10.1145/1718918.1718962,
author = {Hecht, Brent J. and Gergle, Darren},
title = {On the "Localness" of User-Generated Content},
year = {2010},
isbn = {9781605587950},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1718918.1718962},
doi = {10.1145/1718918.1718962},
abstract = {The "localness" of participation in repositories of user-generated content (UGC) with geospatial components has been cited as one of UGC's greatest benefits. However, the degree of localness in major UGC repositories such as Flickr and Wikipedia has never been examined. We show that over 50 percent of Flickr users contribute local information on average, and over 45 percent of Flickr photos are local to the photographer. Across four language editions of Wikipedia, however, we find that participation is less local. We introduce the spatial content production model (SCPM) as a possible factor in the localness of UGC, and discuss other theoretical and applied implications.},
booktitle = {Proceedings of the 2010 ACM Conference on Computer Supported Cooperative Work},
pages = {229–232},
numpages = {4},
keywords = {wikipedia, local, flickr, user behavior, volunteered geographic information, user-generated content, multilingual},
location = {Savannah, Georgia, USA},
series = {CSCW '10}
