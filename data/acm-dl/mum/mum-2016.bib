@inproceedings{10.1145/3012709.3017603,
author = {Ferreira, Denzil and Schuss, Christian and Luo, Chu and Goncalves, Jorge and Kostakos, Vassilis and Rahkonen, Timo},
title = {Indoor Light Scavenging on Smartphones},
year = {2016},
isbn = {9781450348607},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3012709.3017603},
doi = {10.1145/3012709.3017603},
abstract = {There is a limited amount of scavenging alternatives for smartphones. We assess the feasibility of using indoor light to extend smartphones' battery life. We build a prototype charger that demonstrates that indoor light scavenging is a practical method that can substantially extend battery life on smartphones. The results show that it is feasible and practical to extend battery life with this energy harvesting method. We finally discuss certain obstacles that need to be overcome, especially the redesign of operating systems to account for energy harvesting.},
booktitle = {Proceedings of the 15th International Conference on Mobile and Ubiquitous Multimedia},
pages = {369–371},
numpages = {3},
keywords = {emerging technologies, power management, energy-aware systems},
location = {Rovaniemi, Finland},
series = {MUM '16}
}

@inproceedings{10.1145/3012709.3017604,
author = {Mekonnen, Tenager and Harjula, Erkki and Ylianttila, Mika},
title = {Energy Efficient Motion Detection in a High-Resolution Wireless Surveillance Camera Node},
year = {2016},
isbn = {9781450348607},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3012709.3017604},
doi = {10.1145/3012709.3017604},
abstract = {In this demo we present an energy efficient motion detection mechanism for a high-resolution wireless (battery powered) surveillance camera node. We use Raspberry Pi (RPi) as a camera sensor node to demonstrate two scenarios. In the first scenario, the RPi performs motion detection using Pyro-electric InfraRed (PIR) sensor, while in the latter we use an external low-power node to keep the RPi in low power mode and at the same time detect motion using PIR sensor. The main contributions of this demo are the power management implementation, a surveillance application and the evaluation of power consumption between the two scenarios.},
booktitle = {Proceedings of the 15th International Conference on Mobile and Ubiquitous Multimedia},
pages = {373–375},
numpages = {3},
keywords = {visual sensor nodes, internet of things, wireless multimedia sensor networks, surveillance video, visual networks},
location = {Rovaniemi, Finland},
series = {MUM '16}
}

@inproceedings{10.1145/3012709.3017605,
author = {Pouke, Matti and Koskela, Timo and G\"{u}ne\c{s}, \c{S}an and Matero, Matti and Ojala, Karri and Pajukangas, Jukka and Pietik\"{a}inen, Niko and Ojala, Timo},
title = {3D Visualization of a Public Transportation System},
year = {2016},
