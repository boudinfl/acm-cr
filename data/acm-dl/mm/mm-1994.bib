@inproceedings{10.1145/192593.192642,
author = {Agarwal, N. and Son, S.},
title = {Synchronization of Distributed Multimedia Data in an Application-Specific Manner},
year = {1994},
isbn = {0897916867},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/192593.192642},
doi = {10.1145/192593.192642},
abstract = {One of the distinctive features of multimedia systems is the wide range of applications they intend to cover, stretching the gamut from entertainment to life-critical applications such as real-time remote surgery. In the face of such a wide spectrum of applications, protocols used to deal with various issues in multimedia systems should be adaptable to the application. Synchronization is one of the key characteristics of a multimedia system. In this paper, we propose a mechanism for synchronization of distributed media data, the accuracy of which can be tailored to the intended application. The model is extended for both stored and live multimedia data. The synchronization mechanism we propose ensures synchronization for all possible temporal relations. The performance of our scheme is evaluated and some implementation issues are discussed.},
booktitle = {Proceedings of the Second ACM International Conference on Multimedia},
pages = {141–148},
numpages = {8},
location = {San Francisco, California, USA},
series = {MULTIMEDIA '94}
}

@inproceedings{10.1145/192593.192644,
author = {Waters, K. and Levergood, T.},
