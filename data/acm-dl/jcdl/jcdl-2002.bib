author = {Soergel, Dagobert},
title = {Thesauri and Ontologies in Digital Libraries: 1. Structure and Use in Knowledge-Based Assistance to Users},
year = {2002},
isbn = {1581135130},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
