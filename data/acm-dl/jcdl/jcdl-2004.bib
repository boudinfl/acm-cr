@inproceedings{10.1145/996350.996409,
author = {Parker, Charles L.},
title = {A Tree-Based Method for Fast Melodic Retrieval},
year = {2004},
isbn = {1581138326},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/996350.996409},
doi = {10.1145/996350.996409},
abstract = {The evolution of aurally queryable melodic databases (so-called query-by-humming systems) has reached a point where retrieval accuracy is relatively high, even at large database sizes. With this accuracy has come a decrease in retrieval speed as methods have become more sophisticated and computationally expensive. In this paper, we turn our attention to heuristically culling songs from our database that are unlikely given a sung query, in hopes that we can increase speed by reducing the number of matching computations necessary to reach the proper target song.},
booktitle = {Proceedings of the 4th ACM/IEEE-CS Joint Conference on Digital Libraries},
pages = {254–255},
numpages = {2},
keywords = {melodic retrieval, query-by-humming},
location = {Tuscon, AZ, USA},
