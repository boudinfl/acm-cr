}

@inproceedings{10.1145/1738826.1738893,
author = {Buttfield-Addison, Paris and Lueg, Christopher and Manning, Jonathon},
title = {The Pile of Least Effort: Supporting Lived Document Management Practices},
year = {2009},
isbn = {9781605588544},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1738826.1738893},
