}

@inproceedings{10.5555/1108368.1108421,
author = {Moyes, Jackie and Buur, Jacob and Jarrett, Caroline and Ehn, Pelle and Howard, Steven and Brereton, Margot},
title = {Book Smarts Meet Street Smarts: The Best of Both Worlds},
year = {2005},
isbn = {1595932224},
publisher = {Computer-Human Interaction Special Interest Group (CHISIG) of Australia},
address = {Narrabundah, AUS},
abstract = {This panel will discuss how academia can contribute to industry practice and how industry practitioners can contribute to academia. We will focus in particular on how theories and practices are formed and shaped in different settings. We will discuss when academic theories and practices help industry, when and why they are discarded, and how they are transformed in industry settings.},
