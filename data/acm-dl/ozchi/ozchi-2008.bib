@inproceedings{10.1145/1517744.1517790,
author = {Browning, David and Bidwell, Nicola J and Hardy, Dianna and Standley, P-M},
title = {Rural Encounters: Cultural Translations through Video},
year = {2008},
isbn = {0980306345},
