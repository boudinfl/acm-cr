address = {New York, NY, USA},
url = {https://doi.org/10.1145/147001.147052},
doi = {10.1145/147001.147052},
abstract = {We report results from an empirical study of how people access online technical documentation. The results are used to evaluate the concept of an active help system as an aid to improved access. The study showed that the differences amongst users limit the potential for active help, and that help in making tactical choices amongst access methods offers more promise for improved user access.},
