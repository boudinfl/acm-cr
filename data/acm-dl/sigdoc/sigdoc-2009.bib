@inproceedings{10.1145/1621995.1622052,
author = {Slattery, Shaun P.},
title = {"edit This Page": The Socio-Technological Infrastructure of a Wikipedia Article},
year = {2009},
isbn = {9781605585598},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1621995.1622052},
doi = {10.1145/1621995.1622052},
abstract = {Networked environments, such as wikis, are commonly used to support work, including the collaborative authoring of information and "fact-building." In networked environments, the activity of fact-building is mediated not only by the technological features of the interface, but also by the social conventions of the community it supports. This paper examines the social and technological features of a Wikipedia article in order to understand how these features help mediate the activity of fact-building and highlights the need for communication designers to consider the goals and needs of the communities for which they design.},
booktitle = {Proceedings of the 27th ACM International Conference on Design of Communication},
pages = {289–296},
numpages = {8},
keywords = {infrastructure, activity theory, wiki, wikipedia},
location = {Bloomington, Indiana, USA},
series = {SIGDOC '09}
}

@inproceedings{10.1145/1621995.1622053,
author = {McNely, Brian},
title = {Backchannel Persistence and Collaborative Meaning-Making},
year = {2009},
isbn = {9781605585598},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1621995.1622053},
doi = {10.1145/1621995.1622053},
abstract = {Digital backchannel communication has become an increasingly important area of study for researchers and practitioners in several fields. From the emergence of wifi-enabled Internet Relay Chat (IRC) to contemporary instances of microblogging and SMS messaging, the role of digital backchannels in enabling collaborative affordances has received much recent attention. As backchannel communication continues to become more prevalent at professional conferences, in educational curricula, and in organizational settings, robust frameworks for understanding the role of backchannel environments in collaborative meaning-making are needed. Drawing upon cultural-historical activity theory and actor network theory, this paper explores the development of backchannel persistence through microblogging platforms, and suggests an approach to studying the collaborative affordances of backchannel communication by focusing on the related concepts of mobilization and recursive writing collaboration.},
booktitle = {Proceedings of the 27th ACM International Conference on Design of Communication},
pages = {297–304},
numpages = {8},
keywords = {backchannel, mobilization, twitter, persistence, recursion, microblogging},
location = {Bloomington, Indiana, USA},
series = {SIGDOC '09}
}

