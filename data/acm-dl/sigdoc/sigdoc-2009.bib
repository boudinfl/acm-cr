@inproceedings{10.1145/1621995.1621998,
author = {Janeiro, Jordan and Barbosa, Simone Diniz Junqueira and Springer, Thomas and Schill, Alexander},
title = {Enhancing User Interface Design Patterns with Design Rationale Structures},
year = {2009},
isbn = {9781605585598},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1621995.1621998},
doi = {10.1145/1621995.1621998},
abstract = {User interface design and development activities are typically costly and, despite many research efforts, still lack adequate support tools to make them more efficient. The use of guidelines and principles have been mostly replaced by the use of user interface design patterns, collected in libraries that aim to encompass both general and domain-specific user interface design knowledge. However, such patterns are represented mostly in natural language and other representations that cannot be easily computed. This paper presents an approach to represent user interface design patterns in a knowledge base which, by bringing together semi-structured user interface design patterns and design rationale models, can help designers to search, compare, and reuse proven design solutions in their projects.},
booktitle = {Proceedings of the 27th ACM International Conference on Design of Communication},
pages = {9–16},
numpages = {8},
keywords = {knowledge base, semantic match, argumentation model, user interface design patterns, design rationale, information retrieval},
location = {Bloomington, Indiana, USA},
series = {SIGDOC '09}
}

