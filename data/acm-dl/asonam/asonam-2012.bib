@inproceedings{10.1109/ASONAM.2012.119,
author = {Hajibagheri, Alireza and Alvari, Hamidreza and Hamzeh, Ali and Hashemi, Sattar},
title = {Community Detection in Social Networks Using Information Diffusion},
year = {2012},
isbn = {9780769547992},
publisher = {IEEE Computer Society},
address = {USA},
url = {https://doi.org/10.1109/ASONAM.2012.119},
doi = {10.1109/ASONAM.2012.119},
abstract = {Discovering communities in popular social networks like Facebook has been receiving significant attentions recently. In this paper, inspired from real life, we have addressed the community detection problem by a framework based on Information Diffusion Model and Game Theory. In this approach, we consider each node of the social network as a selfish agent which has interactions with its neighbors and tries to maximize its total utility (i.e. received information). Finally community structure of the graph reveals after reaching to the local Nash equilibrium of the game. Experimental results on the benchmark social media datasets, synthetic and real world graphs demonstrate that our method is superior compared with the other state-of-the-art methods.},
booktitle = {Proceedings of the 2012 International Conference on Advances in Social Networks Analysis and Mining (ASONAM 2012)},
pages = {702–703},
numpages = {2},
keywords = {community structure, social network, information diffusion, nash equilibrium, game theory},
series = {ASONAM '12}
}

@inproceedings{10.1109/ASONAM.2012.120,
author = {Cruz, Juan David and Bothorel, Cecile and Poulet, Francois},
title = {Layout Algorithm for Clustered Graphs to Analyze Community Interactions in Social Networks},
year = {2012},
isbn = {9780769547992},
publisher = {IEEE Computer Society},
address = {USA},
url = {https://doi.org/10.1109/ASONAM.2012.120},
doi = {10.1109/ASONAM.2012.120},
abstract = {Most of the layout algorithms for clustered graphs have been designed to differentiate the groups within the graph, however they do not take into account the interactions between such groups. Identifying these interactions allows to understand how the different communities exchange messages or information, and allows the social network researcher to identify key actors, social roles and paths from one community to another. These interactions are performed by actors who actually share links with different communities. Thus, dividing the set of nodes into those who interact with several communities and those who only interact with their own communities, will give some insight about how the communities interact.},
booktitle = {Proceedings of the 2012 International Conference on Advances in Social Networks Analysis and Mining (ASONAM 2012)},
pages = {704–705},
numpages = {2},
keywords = {Force, Clustering algorithms, Communities, Clustered graphs layout, Social network services, Algorithm design and analysis, communities interaction, Visualization, central nodes identification, Layout},
series = {ASONAM '12}
}

