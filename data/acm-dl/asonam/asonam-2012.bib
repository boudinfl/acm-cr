@inproceedings{10.1109/ASONAM.2012.146,
author = {Nounou, Mohamed and Nounou, Hazem and Meskin, Nader and Datta, Aniruddha},
title = {Wavelet-Based Multiscale Filtering of Genomic Data},
year = {2012},
isbn = {9780769547992},
publisher = {IEEE Computer Society},
address = {USA},
url = {https://doi.org/10.1109/ASONAM.2012.146},
doi = {10.1109/ASONAM.2012.146},
abstract = {Measured biological data are a rich source of information about the biological phenomena they represent. For example, time-series genomic or metabolic micro array data can be used to construct dynamic genetic regulatory network models, which can be used to better understand the biological system and to design intervention strategies to cure or manage major diseases. Unfortunately, biological measurements are usually highly contaminated with errors that mask the important features in the data. Therefore, these noisy measurements need to be filtered to enhance their usefulness in practice. Wavelet-based multiscale filtering has been shown to be a powerful data analysis and denoising tool. In this work, different batch as well as online multiscale filtering techniques are used to filter biological data contaminated with white noise. The performances of these multiscale filtering techniques are demonstrated and compared to those of some conventional low pass filters using simulated time series metabolic data. The results of this comparative study show that significant improvement can be achieved using multiscale filtering over conventional filtering methods.},
booktitle = {Proceedings of the 2012 International Conference on Advances in Social Networks Analysis and Mining (ASONAM 2012)},
pages = {804–809},
numpages = {6},
keywords = {Multiscale filtering, Genomic Data, Wavelets},
series = {ASONAM '12}
}

@inproceedings{10.1109/ASONAM.2012.147,
author = {Markon, Sandor and Maekawa, Satoshi and Onat, Ahmet},
title = {Robot-Assisted Medical Visualization with Floating Images},
year = {2012},
isbn = {9780769547992},
publisher = {IEEE Computer Society},
address = {USA},
url = {https://doi.org/10.1109/ASONAM.2012.147},
doi = {10.1109/ASONAM.2012.147},
abstract = {Accessing volumetric medical data requires advanced visualization techniques. One approach is to show slices in situ, by projecting images into space at the proper position. We propose using floating images, that is, undistorted real images of displays, appearing at the desired position and orientation. Such floating images are made possible by the new optical device eDCRA' invented at NICT in Japan. To enhance the freedom of the user, we have developed a robot-assisted interactive visualization system, where the user can ehold' the image slice in her/his hand, and freely change its position and orientation, thus inspect any part of the volumetric data set as desired. The hand position is detected by a stereo camera and image processing. We describe the structure and operation of our system, and show samples of its usage.},
booktitle = {Proceedings of the 2012 International Conference on Advances in Social Networks Analysis and Mining (ASONAM 2012)},
pages = {810–814},
numpages = {5},
keywords = {floating images, visualization, volume slicing, robot arm},
