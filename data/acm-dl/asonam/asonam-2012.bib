publisher = {IEEE Computer Society},
address = {USA},
url = {https://doi.org/10.1109/ASONAM.2012.127},
doi = {10.1109/ASONAM.2012.127},
abstract = {We study the computation capability of the BP cycles counting algorithm in random networks by estimating the required convergence error to find the maximum number of points in the cycles distribution which helps to interpolate accurately the cycles counts for different sizes. The algorithm shows that the convergence error required to get all possible points is almost constant through all possible connection probability values. The number of points possible can not reach the maximum even with high convergence error values.},
